{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome against localhost",
        "type": "chrome",
        "request": "launch",
        "url": "http://127.0.0.1:9000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "trace": true,
        "sourceMapPathOverrides": {
          "webpack:///./*": "${webRoot}/*"
        }
      }
    ]
  }